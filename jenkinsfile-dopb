node {
    stage ('checkout') {
        checkout scm
    }
    stage ('build') {
       copyArtifacts projectName: 'devopticsa/master'
       gateConsumesArtifact file: 'plugin-a.txt'
        sh "git rev-parse HEAD > plugin-b.txt"
        archiveArtifacts artifacts: 'plugin-b.txt'
    }
    stage ('produce') {
        gateProducesArtifact file: 'plugin-b.txt'
    }
}